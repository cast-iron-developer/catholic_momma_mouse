{
  "name": "catholic-momma-mouse-client",
  "description": "This is a simple, Faith Meets Fantasy meant to show the Christological elements that can be found within both the classic and newer Disney projects.",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "svelte-kit sync && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:start": "docker compose up"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "mdsvex": "^0.12.3",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "@types/node": "^22.10.7",
    "drizzle-orm": "^0.38.4",
    "postgres": "^3.4.5"
  }
}
